{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 5 versions",
                        "ie >= 8"
                    ]
                },
                "modules": false,
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ]
    ],
    "plugins": [
        [
            "@babel/transform-runtime"
        ],
        [
            "extract",
            {
                "name": "my_babel_plugin"
            }
        ]
    ]
}